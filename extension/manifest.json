{
  "manifest_version": 3,
  "name": "RealityCheck AI",
  "version": "1.0",
  "description": "Detect deepfakes on YouTube",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "https://www.youtube.com/*",
    "http://127.0.0.1:8000/*"
  ],
  
"content_scripts": [
  {
    "matches": ["*://*.youtube.com/*"],
    "js": ["content.js"]
  }
]
}